{
  "name": "vioflow-mam-monorepo",
  "version": "1.0.0",
  "private": true,
  "description": "Vioflow MAM - Monorepo 项目结构",
  "workspaces": [
    "apps/*",
    "backend"
  ],
  "scripts": {
    "dev:web": "cd apps/web && npm run dev",
    "dev:backend": "cd backend && npm run start:dev",
    "dev": "npm run dev:web & npm run dev:backend",
    "build:web": "cd apps/web && npm run build",
    "build:backend": "cd backend && npm run build",
    "build": "npm run build:web && npm run build:backend",
    "install:all": "npm install && cd apps/web && npm install && cd ../../backend && npm install",
    "lint:web": "cd apps/web && npm run lint",
    "lint:backend": "cd backend && npm run lint",
    "test:web": "cd apps/web && npm run test",
    "test:backend": "cd backend && npm run test"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "."
  },
  "keywords": [
    "monorepo",
    "vioflow",
    "mam",
    "video-management"
  ],
  "author": "",
  "license": "PRIVATE"
}
